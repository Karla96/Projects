@model Projekt.DAL.Models.FoodFilter

@using (Html.BeginForm())
{
<div class="form-inline" style="margin-bottom: 30px;">
    <div class="form-group">
        @Html.TextBoxFor(model => model.Name, htmlAttributes: new { @class = "form-control", placeholder = "Food Name" })
    </div>
    <div class="form-group">
        <button type="button" onclick="applyFilter(this)" class="btn btn-primary">Search</button>
    </div>
</div>


}
